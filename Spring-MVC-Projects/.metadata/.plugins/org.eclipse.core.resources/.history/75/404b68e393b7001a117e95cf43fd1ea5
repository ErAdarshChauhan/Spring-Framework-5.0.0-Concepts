		<script type="text/javascript">
		$(document).ready(function(){
			$('a').mouseenter(function(){
				$(this).css({
					"background-color":"red",
					"color":"white"
				});
				
			});
			
			$('a').mouseleave(function(){
				$(this).css({
					"background-color":"olive",
					"color":"red"
				});
				
			});
					

		});

		</script>
